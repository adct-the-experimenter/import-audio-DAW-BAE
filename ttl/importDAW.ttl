

#The full description of the plugin is in this file, which is linked to from manifest.ttl. 
#This is done so the host only needs to scan the relatively small manifest.ttl files to quickly discover all plugins.

@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

#URI of plugin, must match one in file manifest.ttl.in
<http://adfxz.com/plugins/importDAWBAE#Mono>
        a lv2:Plugin ,
                lv2:ExportAudioBAEPlugin ;
                
#project information on plugin
lv2:project <http://lv2plug.in/ns/lv2> ;

#name of the plugin
doap:name "Export Audio To Binaural Audio Editor (Mono)" ;
        doap:license <http://opensource.org/licenses/isc> ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port
					[
						a lv2:AudioPort ,
						lv2:InputPort ;
						lv2:index 0 ;
						lv2:symbol "in" ;
						lv2:name "In"
					] , 
					[
						a lv2:AudioPort ,
						lv2:OutputPort ;
						lv2:index 1 ;
						lv2:symbol "out" ;
						lv2:name "Out"
					] .


#URI of plugin, must match one in file manifest.ttl.in
<http://adfxz.com/plugins/importDAWBAE#Stereo>
        a lv2:Plugin ,
                lv2:ExportAudioBAEPlugin ;
                
#project information on plugin
lv2:project <http://lv2plug.in/ns/lv2> ;

#name of the plugin
doap:name "Export Audio To Binaural Audio Editor (Stereo)" ;
        doap:license <http://opensource.org/licenses/isc> ;
        lv2:optionalFeature lv2:hardRTCapable ;
        lv2:port
					[
						a lv2:AudioPort ,
						lv2:InputPort ;
						lv2:index 0 ;
						lv2:symbol "in_left" ;
						lv2:name "InLeft"
					] ,
					[
						a lv2:AudioPort ,
						lv2:InputPort ;
						lv2:index 1 ;
						lv2:symbol "in_right" ;
						lv2:name "InRight"
					] , 
					[
						a lv2:AudioPort ,
						lv2:OutputPort ;
						lv2:index 2 ;
						lv2:symbol "out_left" ;
						lv2:name "OutLeft"
					],
					[
						a lv2:AudioPort ,
						lv2:OutputPort ;
						lv2:index 3 ;
						lv2:symbol "out_right" ;
						lv2:name "OutRight"
					] .
